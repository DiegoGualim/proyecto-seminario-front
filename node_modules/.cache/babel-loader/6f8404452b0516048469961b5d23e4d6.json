                celular: '',\n                telefono_casa: '',\n                expediente: '',\n                temperatura: '',\n                tipo_consulta: '',\n                fecha: ''\n            },\n            show: true,\n            buttonDisabled: false\n        }\n    },\n    methods: {\n        async onSubmit(event) {\n            event.preventDefault()\n            this.buttonDisabled = true\n            console.log('ON SUBMIT -> ', this.form)\n            //this.form.value = this.form.value.replace(/,/g, '')\n            const reconsultation = await requestHandler\n                .async_fetch(\n                    `${process.env.VUE_APP_BACKEND}/api/v1/oldconsultation/updateOldConsultation`,\n                    requestOptions.put(this.form)\n                )\n                .then(() => {\n                    this.showToast()\n                    this.buttonDisabled = false\n                })\n            EventBus.$emit('updateCompraTable')\n            console.log(reconsultation)\n            ;(this.form.id = ''),\n                (this.form.carnet = ''),\n                (this.form.nombre = ''),\n                (this.form.apellido = ''),\n                (this.form.edad = ''),\n                (this.form.direccion = ''),\n                (this.form.celular = ''),\n                (this.form.telefono_casa = ''),\n                (this.form.expediente = ''),\n                (this.form.temperatura = ''),\n                (this.form.tipo_consulta = ''),\n                (this.form.fecha = ''),\n                // Trick to reset/clear native browser form validation state\n                (this.show = false)\n            this.$nextTick(() => {\n                this.show = true\n            })\n        },\n        onReset(event) {\n            event.preventDefault()\n            // Reset our form values\n            ;(this.form.id = ''),\n                (this.form.carnet = ''),\n                (this.form.nombre = ''),\n                (this.form.apellido = ''),\n                (this.form.edad = ''),\n                (this.form.direccion = ''),\n                (this.form.celular = ''),\n                (this.form.telefono_casa = ''),\n                (this.form.expediente = ''),\n                (this.form.temperatura = ''),\n                (this.form.tipo_consulta = ''),\n                (this.form.fecha = ''),\n                // Trick to reset/clear native browser form validation state\n                (this.show = false)\n            this.$nextTick(() => {\n                this.show = true\n            })\n        },\n        showToast() {\n            // Use a shorter name for this.$createElement\n            const h = this.$createElement\n            // Increment the toast count\n            // Create the message\n            const vNodesMsg = h('p', { class: ['text-center', 'mb-0'] }, [\n                h('strong', 'Compra Editada con Éxito')\n            ])\n            // Pass the VNodes as an array for message and title\n            this.$bvToast.toast([vNodesMsg], {\n                solid: true,\n                variant: 'info',\n                toaster: 'b-toaster-top-right',\n                noCloseButton: true\n            })\n        },\n        updateData(item) {\n            console.log('date -> ', item)\n            this.form = item\n            let fecha = item.fecha.split('/')\n            const dia = fecha[0]\n            const mes = fecha[1]\n            const ano = fecha[2]\n            this.form.fecha = `${ano}-${mes}-${dia}`\n            console.log(item.id, fecha)\n        }\n    },\n    async created() {\n        EventBus.$on('editCompraTable', (item) => {\n            console.log('evento', item)\n            this.updateData(item)\n            this.show = false\n            this.$nextTick(() => {\n                this.show = true\n            })\n        })\n    },\n    beforeDestroy() {\n        EventBus.$off('editCompraTable')\n    }\n}\n/**\n * style=\"background-color:#6E6658; padding:3rem;\"\n * https://codepen.io/wfigueiredo/pen/BVmzzy\n */\n</script>\n"],"sourceRoot":"src/components/OldConsulta"}]}