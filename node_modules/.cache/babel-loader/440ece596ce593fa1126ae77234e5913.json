bel-for=\"input-1\"\n          >\n            <b-form-input\n              id=\"input-1\"\n              v-model=\"form.bill_no\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n      </b-row>\n      <b-row cols=\"1\" cols-sm=\"1\" cols-md=\"1\" cols-lg=\"2\" cols-xl=\"2\">\n        <b-col\n          ><b-form-group\n            id=\"input-group-2\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Débito\"\n            label-for=\"input-2\"\n          >\n            <b-form-input\n              id=\"input-2\"\n              v-model=\"form.debit\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n        <b-col\n          ><b-form-group\n            id=\"input-group-3\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Crédito\"\n            label-for=\"input-3\"\n          >\n            <b-form-input\n              id=\"input-3\"\n              v-model=\"form.credit\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n        <!-- <b-col\n          ><b-form-group\n            id=\"input-group-4\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Total\"\n            label-for=\"input-4\"\n          >\n            <b-form-input\n              id=\"input-4\"\n              v-model=\"form.total\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col> -->\n      </b-row>\n\n      <div class=\"mt-3\">\n        <b-button\n          :disabled=\"buttonDisabled\"\n          type=\"submit\"\n          variant=\"primary bg-success\"\n          class=\"mt-4\"\n          style=\"min-width: 14rem\"\n          >Insertar Registro</b-button\n        >\n      </div>\n    </b-form>\n  </div>\n</template>\n\n<script>\nimport { requestHandler } from \"@/helpers/requestHandler\";\nimport { requestOptions } from \"@/helpers/requestOptions\";\nimport EventBus from \"@/bus\";\n\nexport default {\n  name: \"Form\",\n  data() {\n    return {\n      form: {\n        bill_no: \"\",\n        debit: \"\",\n        credit: \"\",\n        total: \"\",\n        initial_capital: \"\",\n      },\n      show: true,\n      buttonDisabled: false,\n    };\n  },\n  methods: {\n    async onSubmit(event) {\n      event.preventDefault();\n      this.buttonDisabled = true;\n      console.log(this.form);\n      this.form.debit = this.form.debit.replace(/,/g, \"\");\n      this.form.credit = this.form.credit.replace(/,/g, \"\");\n      this.form.total = this.form.total.replace(/,/g, \"\");\n      const reconsultation = await requestHandler\n        .async_fetch(\n          `${process.env.VUE_APP_BACKEND}/api/v1/purchases`,\n          requestOptions.put(this.form)\n        )\n        .then(() => {\n          this.showToast();\n          this.showToastS();\n        });\n      EventBus.$emit(\"updateLibroBancoTable\");\n      this.buttonDisabled = false;\n      console.log(reconsultation);\n      (this.form.bill_no = \"\"),\n        (this.form.debit = \"\"),\n        (this.form.credit = \"\"),\n        (this.form.total = \"\"),\n        (this.form.initial_capital = \"\"),\n        // Trick to reset/clear native browser form validation state\n        (this.show = false);\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      (this.form.bill_no = \"\"),\n        (this.form.debit = \"\"),\n        (this.form.credit = \"\"),\n        (this.form.total = \"\"),\n        (this.form.initial_capital = \"\"),\n        // Trick to reset/clear native browser form validation state\n        (this.show = false);\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n    showToast() {\n      // Use a shorter name for this.$createElement\n      const h = this.$createElement;\n      // Increment the toast count\n      // Create the message\n      const vNodesMsg = h(\"p\", { class: [\"text-center\", \"mb-0\"] }, [\n        h(\"strong\", \"Registro Ingresado con Éxito\"),\n      ]);\n      // Pass the VNodes as an array for message and title\n      this.$bvToast.toast([vNodesMsg], {\n        solid: true,\n        variant: \"info\",\n        toaster: \"b-toaster-top-right\",\n        noCloseButton: true,\n      });\n    },\n  },\n};\n/**\n * style=\"background-color:#6E6658; padding:3rem;\"\n * https://codepen.io/wfigueiredo/pen/BVmzzy\n */\n</script>"],"sourceRoot":"src/components/LibroBanco"}]}