AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Table.vue","sourceRoot":"src/components/Report","sourcesContent":["<template>\n    <b-container fluid>\n        <!-- User Interface controls -->\n        <b-row>\n            <b-col lg=\"6\" class=\"my-1\">\n                <b-form-group\n                    label=\"Ordenar\"\n                    label-for=\"sort-by-select\"\n                    label-cols-sm=\"3\"\n                    label-align-sm=\"right\"\n                    label-size=\"sm\"\n                    class=\"mb-0\"\n                    v-slot=\"{ ariaDescribedby }\"\n                >\n                    <b-input-group size=\"sm\">\n                        <b-form-select\n                            id=\"sort-by-select\"\n                            v-model=\"sortBy\"\n                            :options=\"sortOptions\"\n                            :aria-describedby=\"ariaDescribedby\"\n                            class=\"w-75\"\n                        >\n                            <template #first>\n                                <option value=\"\">-- ninguno --</option>\n                            </template>\n                        </b-form-select>\n\n                        <b-form-select\n                            v-model=\"sortDesc\"\n                            :disabled=\"!sortBy\"\n                            :aria-describedby=\"ariaDescribedby\"\n                            size=\"sm\"\n                            class=\"w-25\"\n                        >\n                            <option :value=\"false\">Asc</option>\n                            <option :value=\"true\">Desc</option>\n                        </b-form-select>\n                    </b-input-group>\n                </b-form-group>\n            </b-col>\n\n            <b-col lg=\"6\" class=\"my-1\">\n                <b-form-group\n                    label=\"Descargar Excel\"\n                    label-for=\"sort-by-select\"\n                    label-cols-sm=\"3\"\n                    label-align-sm=\"right\"\n                    label-size=\"sm\"\n                    class=\"mb-0\"\n                >\n                    <b-input-group size=\"sm\">\n                        <b-input-group-append style=\"height: 1.934rem\">\n                            <b-button class=\"excelbutton\" @click=\"this.exportExcel\" variant=\"primary \"\n                                >Excel\n                                <span class=\"material-icons\">\n                                    download_for_offline\n                                </span></b-button\n                            >\n                        </b-input-group-append>\n                    </b-input-group>\n                </b-form-group>\n            </b-col>\n\n            <b-col lg=\"6\" class=\"my-1\">\n                <b-form-group\n                    label=\"Filtrar\"\n                    label-for=\"filter-input\"\n                    label-cols-sm=\"3\"\n                    label-align-sm=\"right\"\n                    label-size=\"sm\"\n                    class=\"mb-0\"\n                >\n                    <b-input-group size=\"sm\">\n                        <b-form-input\n                            id=\"filter-input\"\n                            v-model=\"filter\"\n                            type=\"search\"\n                            placeholder=\"Escribe para buscar\"\n                        ></b-form-input>\n\n                        <b-input-group-append>\n                            <b-button :disabled=\"!filter\" @click=\"filter = ''\">Limpiar</b-button>\n                        </b-input-group-append>\n                    </b-input-group>\n                </b-form-group>\n            </b-col>\n\n            <b-col lg=\"6\" class=\"my-1\"> </b-col>\n\n            <b-col lg=\"12\" class=\"my-1\">\n                <b-form-group id=\"input-group-4\">\n                    <b-form-select\n                        id=\"input-4\"\n                        v-model=\"month\"\n                        :options=\"optionsFormSelect\"\n                        required\n                        @change=\"onChangeMonth($event)\"\n                    ></b-form-select>\n                </b-form-group>\n            </b-col>\n\n            <b-col sm=\"5\" md=\"6\" class=\"my-1\">\n                <b-form-group\n                    label=\"Por página\"\n                    label-for=\"per-page-select\"\n                    label-cols-sm=\"6\"\n                    label-cols-md=\"4\"\n                    label-cols-lg=\"3\"\n                    label-align-sm=\"right\"\n                    label-size=\"sm\"\n                    class=\"mb-0\"\n                >\n                    <b-form-select\n                        id=\"per-page-select\"\n                        v-model=\"perPage\"\n                        :options=\"pageOptions\"\n                        size=\"sm\"\n                    ></b-form-select>\n                </b-form-group>\n            </b-col>\n\n            <b-col sm=\"7\" md=\"6\" class=\"my-1\">\n                <b-pagination\n                    v-model=\"currentPage\"\n                    :total-rows=\"totalRows\"\n                    :per-page=\"perPage\"\n                    align=\"fill\"\n                    size=\"sm\"\n                    class=\"my-0\"\n                ></b-pagination>\n            </b-col>\n        </b-row>\n\n        <!-- Main table element -->\n        <b-table\n            :items=\"items\"\n            :fields=\"fields\"\n            :current-page=\"currentPage\"\n            :per-page=\"perPage\"\n            :filter=\"filter\"\n            :filter-included-fields=\"filterOn\"\n            :sort-by.sync=\"sortBy\"\n            :sort-desc.sync=\"sortDesc\"\n            :sort-direction=\"sortDirection\"\n            stacked=\"md\"\n            striped\n            show-empty\n            responsive=\"\"\n            small\n            head-variant=\"dark\"\n            @filtered=\"onFiltered\"\n            class=\"mt-3\"\n        >\n            <template #cell(debit)=\"row\">\n                {{\n                    parseFloat(row.value).toLocaleString('en-US', {\n                        minimumFractionDigits: 2,\n                        maximumFractionDigits: 2,\n                    })\n                }}\n            </template>\n            <template #cell(credit)=\"row\">\n                {{\n                    parseFloat(row.value || 0).toLocaleString('en-US', {\n                        minimumFractionDigits: 2,\n                        maximumFractionDigits: 2,\n                    })\n                }}\n            </template>\n            <template #cell(value)=\"row\">\n                {{\n                    parseFloat(row.value).toLocaleString('en-US', {\n                        minimumFractionDigits: 2,\n                        maximumFractionDigits: 2,\n                    })\n                }}\n            </template>\n            <!-- <template #cell(actions)=\"row\">\n        <b-button size=\"sm\" @click=\"editRow(row.item, row.index, $event.target)\">\n          Editfilter\n        </b-button>\n        <b-button size=\"sm\" @click=\"info(row.item, row.index, $event.target)\" >\n          Delete\n        </b-button>\n      </template> -->\n        </b-table>\n        <!-- <b-card class=\"mt-3\" header=\"Form Data Result\">\n      <pre class=\"m-0\">{{ filteredItems }}</pre>\n    </b-card> -->\n        <p>El total del mes seleccionado es: Q.{{ totalSalesMonth }}</p>\n    </b-container>\n</template>\n\n<script>\nimport { requestHandler } from '@/helpers/requestHandler';\nimport { requestOptions } from '@/helpers/requestOptions';\nimport EventBus from '@/bus';\n\n//import XLSX from \"xlsx\";\n\nexport default {\n    data() {\n        return {\n            items: [],\n            fields: [\n                /* {\n          key: \"id\",\n          sortable: true,\n          label: \"ID\",\n        }, */\n                {\n                    key: 'No',\n                    sortable: true,\n                    label: 'No.',\n                } /*\n        {\n          key: \"bill_series\",\n          sortable: true,\n          label: \"Serie Factura\",\n        },\n        {\n          key: \"provider_nit\",\n          sortable: true,\n          label: \"NIT Proveedor\",\n        }, */,\n                {\n                    key: 'fecha',\n                    sortable: true,\n                    label: 'Fecha',\n                },\n                {\n                    key: 'no_factura',\n                    sortable: true,\n                    label: 'No.Factura',\n                },\n                {\n                    key: 'categoria',\n                    sortable: true,\n                    label: 'Categoria',\n                },\n                {\n                    key: 'descripcion',\n                    sortable: true,\n                    label: 'Descripción',\n                },\n                {\n                    key: 'Cantidad_Comprada',\n                    sortable: true,\n                    label: 'Cantidad Comprada',\n                },\n                {\n                    key: 'price',\n                    sortable: true,\n                    label: 'Precio',\n                } /* ,\n        {\n          key: \"total\",\n          sortable: true,\n          label: \"Total\",\n        }  */ /* ,\n        { key: \"actions\", label: \"Actions\" }, */,\n            ],\n            totalRows: 1,\n            currentPage: 1,\n            perPage: 5,\n            pageOptions: [5, 10, 15, { value: 100, text: 'Mostrar muchos' }],\n            sortBy: 'No',\n            sortDesc: true,\n            sortDirection: 'desc',\n            filter: null,\n            filterOn: [],\n            filteredItems: [],\n            initialValue: 0,\n            optionsFormSelect: [\n                { value: null, disabled: true, text: 'Seleccione un Mes para filtrar' },\n                { value: '01', text: 'Enero' },\n                { value: '02', text: 'Febrero' },\n                { value: '03', text: 'Marzo' },\n                { value: '04', text: 'Abril' },\n                { value: '05', text: 'Mayo' },\n                { value: '06', text: 'Junio' },\n                { value: '07', text: 'Julio' },\n                { value: '08', text: 'Agosto' },\n                { value: '09', text: 'Septiembre' },\n                { value: '10', text: 'Octubre' },\n                { value: '11', text: 'Noviembre' },\n                { value: '12', text: 'Diciembre' },\n            ],\n            month: null,\n            totalSalesMonth: '0.00',\n        };\n    },\n    computed: {\n        sortOptions() {\n            // Create an options list from our fields\n            return this.fields\n                .filter(f => f.sortable)\n                .map(f => {\n                    return { text: f.label, value: f.key };\n                });\n        },\n    },\n    async mounted() {\n        // Set the initial number of items\n        await this.fetchData();\n        this.totalRows = this.items.length;\n        this.filteredItems = this.items;\n    },\n    async beforeCreate() {},\n    methods: {\n        resetInfoModal() {\n            this.infoModal.title = '';\n            this.infoModal.content = '';\n        },\n        onChangeMonth(eventChange) {\n            console.log('EVENTCHANGE ' + eventChange);\n            this.filterOn = ['fecha'];\n            const hoy = new Date(Date.now());\n            console.log(hoy);\n            this.filter = `/${eventChange.toString()}/${hoy.getFullYear()}`;\n            this.filter2 = `/${eventChange.toString()}/${hoy.getFullYear()}`;\n\n            let year = hoy.getFullYear();\n            let month = eventChange.toString();\n            //eventChange.toString() pasar el mes\n            //hoy.getFullYear() pasar el ano\n            //this.filter = `${hoy.getFullYear()}-${eventChange.toString()}`;\n            this.getSumaTotal(year, month);\n            console.log(this.filter);\n        },\n        onFiltered(filteredItems) {\n            // Trigger pagination to update the number of buttons/pages due to filtering\n            this.totalRows = filteredItems.length;\n            this.filteredItems = filteredItems;\n            this.currentPage = 1;\n        },\n        // new methods\n        async fetchData() {\n            this.items = await requestHandler.async_fetch(\n                `${process.env.VUE_APP_BACKEND}/api/v1/reportfacturacion`,\n                requestOptions.get()\n            );\n            this.filteredItems = this.items;\n        },\n        async getSumaTotal(year, month) {\n            //console.log('DENTRO DE FUNCION SUMA TOTAL ' + p_fecha_total);\n\n            let jsonFecha = {\n                fecha_factura_month: month,\n                fecha_factura_year: year,\n            };\n\n            console.log('jsonFECHGA');\n            console.log(jsonFecha);\n\n            this.totalSalesMonth = await requestHandler.async_fetch(\n                `${process.env.VUE_APP_BACKEND}/api/v1/reportfacturacion/totalmes`,\n                requestOptions.post(jsonFecha)\n            );\n        },\n        /* exportExcel() {\n      const a = document.createElement(\"a\");\n      document.body.appendChild(a);\n      a.style = \"display: none\";\n\n      var wb = XLSX.utils.book_new();\n\n      wb.Props = {\n        Title: \"Libro De Bancos\",\n        Subject: \"Exportado de datos\",\n        Author: \"Fundación APA\",\n        CreatedDate: new Date(),\n      };\n      wb.SheetNames.push(\"Libro\");\n\n      var ws = XLSX.utils.json_to_sheet(this.filteredItems,{origin: { r: 4, c: 0 }});\n      //ws['A1'].s = {};\n      wb.Sheets[\"Libro\"] = ws;\n      wb.Props\n\n      var wbout = XLSX.write(wb, { bookType: \"xlsx\", type: \"binary\" });\n\n      function s2ab(s) {\n        var buf = new ArrayBuffer(s.length); //convert s to arrayBuffer\n        var view = new Uint8Array(buf); //create uint8array as viewer\n        for (var i = 0; i < s.length; i++) view[i] = s.charCodeAt(i) & 0xff; //convert to octet\n        return buf;\n      }\n\n      const blob = new Blob([s2ab(wbout)], {\n        type: \"application/octet-stream\",\n      });\n      this.fileCSVBlob = window.URL.createObjectURL(blob);\n      a.href = this.fileCSVBlob;\n      a.download = `LibroDeBancos.xlsx`;\n      a.click();\n      window.URL.revokeObjectURL(this.fileCSVBlob);\n    }, */\n        async exportExcel() {\n            const a = document.createElement('a');\n            document.body.appendChild(a);\n            a.style = 'display: none';\n\n            const wbout = await this.postCheckExcel(this.filteredItems);\n            console.log(wbout);\n\n            function s2ab(s) {\n                var buf = new ArrayBuffer(s.length); //convert s to arrayBuffer\n                var view = new Uint8Array(buf); //create uint8array as viewer\n                for (var i = 0; i < s.length; i++) view[i] = s.charCodeAt(i) & 0xff; //convert to octet\n                return buf;\n            }\n\n            const blob = new Blob([s2ab(wbout)], {\n                type: 'application/octet-stream',\n            });\n            this.fileCSVBlob = window.URL.createObjectURL(blob);\n            a.href = this.fileCSVBlob;\n            a.download = `ConsolidadoFacturas3.xlsx`;\n            a.click();\n            window.URL.revokeObjectURL(this.fileCSVBlob);\n        },\n        async postCheckExcel(items) {\n            console.log('ITEMS ' + JSON.stringify(items));\n            const formJson = {\n                items: items,\n                initial_value: this.totalSalesMonth,\n            };\n            const excelstring = await requestHandler.async_fetch_text_response(\n                `${process.env.VUE_APP_BACKEND}/api/v1/reportfacturacion/excel`,\n                requestOptions.post(formJson)\n            );\n            return excelstring;\n        },\n        editRow(item, index, event) {\n            console.log(item, index, event);\n        },\n    },\n    async created() {\n        EventBus.$on('updateLibroBancoTable', reconsultation => {\n            this.fetchData();\n            console.log('evento', reconsultation);\n        });\n    },\n    beforeDestroy() {\n        EventBus.$off('updateLibroBancoTable');\n    },\n};\n/**\n * TODO filter:\"/04/\" and filterOn:Array[1][0]:\"date\"\n */\n</script>\n"]}]}