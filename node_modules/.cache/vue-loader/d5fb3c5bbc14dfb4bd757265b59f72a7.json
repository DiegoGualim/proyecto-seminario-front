p>\n                </b-col>\n                <b-col>\n                    <b-form-group\n                        id=\"input-group-7\"\n                        label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                        label-align-md=\"left\"\n                        label-align-xl=\"left\"\n                        label=\"Telefono Casa\"\n                        label-for=\"input-7\"\n                    >\n                        <b-form-input\n                            id=\"input-7\"\n                            v-model=\"form.telefono_casa\"\n                            placeholder=\"\"\n                            required\n                        ></b-form-input>\n                    </b-form-group>\n                </b-col>\n            </b-row>\n            <b-row cols=\"1\" cols-sm=\"1\" cols-md=\"1\" cols-lg=\"2\" cols-xl=\"2\">\n                <b-col>\n                    <b-form-group\n                        id=\"input-group-7\"\n                        label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                        label-align-md=\"left\"\n                        label-align-xl=\"left\"\n                        label=\"Expediente\"\n                        label-for=\"input-7\"\n                    >\n                        <b-form-input\n                            id=\"input-7\"\n                            v-model=\"form.expediente\"\n                            placeholder=\"\"\n                            required\n                        ></b-form-input>\n                    </b-form-group>\n                </b-col>\n                <b-col>\n                    <b-form-group\n                        id=\"input-group-7\"\n                        label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                  