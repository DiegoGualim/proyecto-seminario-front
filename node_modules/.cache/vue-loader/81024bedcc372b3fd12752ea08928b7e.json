               label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                        label-align-md=\"left\"\n                        label-align-xl=\"left\"\n                        label=\"Nombre\"\n                        label-for=\"input-1\"\n                    >\n                        <b-form-input\n                            id=\"input-1\"\n                            v-model=\"form.nombre\"\n                            placeholder=\"\"\n                            required\n                        ></b-form-input> </b-form-group\n                ></b-col>\n                <b-col lg=\"4\" xl=\"4\"\n                    ><b-form-group\n                        id=\"input-group-2\"\n                        label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                        label-align-md=\"left\"\n                        label-align-xl=\"left\"\n                        label=\"Apellido\"\n                        label-for=\"input-2\"\n                    >\n                        <b-form-input\n                            id=\"input-2\"\n                            v-model=\"form.apellido\"\n                            placeholder=\"\"\n                            required\n                        ></b-form-input> </b-form-group\n                ></b-col>\n                <b-col lg=\"2\" xl=\"2\"\n                    ><b-form-group\n                        id=\"input-group-3\"\n                        label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                        label-align-md=\"left\"\n                        label-align-xl=\"left\"\n                        label=\"Edad\"\n                        label-for=\"input-3\"\n                    >\n                        <b-form-input\n                            id=\"input-3\"\n                            v-model=\"form.edad\"\n                            type=\"number\"\n                            placeholder=\"\"\n                            required\n                        ></b-form-input> </b-form-group\n                ></b-col>\n                <b-col lg=\"2  \" xl=\"2\">\n                    <b-form-group\n                        id=\"input-group-4\"\n                        label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                        label-align-md=\"left\"\n                        label-align-xl=\"left\"\n                        label=\"Temperatura\"\n                        label-for=\"input-4\"\n                    >\n                        <b-form-input\n                            id=\"input-4\"\n                            v-model=\"form.temperatura\"\n                            type=\"number\"\n                            placeholder=\"\"\n                            required\n                        ></b-form-input>\n                    </b-form-group>\n                </b-col>\n            </b-row>\n\n            <b-form-group\n                id=\"input-group-5\"\n                label-align=\"left\"\n                label-align-sm=\"left\"\n                label-align-lg=\"left\"\n                label-align-md=\"left\"\n                label-align-xl=\"left\"\n                label=\"Dirección\"\n                label-for=\"input-5\"\n            >\n                <b-form-input\n                    id=\"input-5\"\n                    v-model=\"form.direccion\"\n                    placeholder=\"\"\n                    required\n                ></b-form-input>\n            </b-form-group>\n\n            <b-row cols=\"1\" cols-sm=\"1\" cols-md=\"1\" cols-lg=\"2\" cols-xl=\"2\">\n                <b-col>\n                    <b-form-group\n                        id=\"input-group-6\"\n                        label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                        label-align-md=\"left\"\n                        label-align-xl=\"left\"\n                        label=\"Celular\"\n                        label-for=\"input-6\"\n                    >\n                        <b-form-input\n                            id=\"input-6\"\n                            type=\"tel\"\n                            v-model=\"form.celular\"\n                            placeholder=\"\"\n                            required\n                        ></b-form-input>\n                    </b-form-group>\n                </b-col>\n                <b-col>\n                    <b-form-group\n                        id=\"input-group-7\"\n                        label-align=\"left\"\n                        label-align-sm=\"left\"\n                        label-align-lg=\"left\"\n                        label-align-md=\"left\"\n                        label-align-xl=\"left\"\n                        label=\"Teléfono de casa\"\n                        label-for=\"input-7\"\n                    >\n                        <b-form-input\n                            id=\"input-7\"\n                            type=\"tel\"\n                            v-model=\"form.telefono_casa\"\n                            placeholder=\"\"\n                            required\n                        ></b-form-input>\n                    </b-form-group>\n                </b-col>\n            </b-row>\n            <b-form-group\n                id=\"input-group-8\"\n                label-align=\"left\"\n                label-align-sm=\"left\"\n                label-align-lg=\"left\"\n                label-align-md=\"left\"\n                label-align-xl=\"left\"\n                label=\"Tipo de clínica\"\n                label-for=\"input-8\"\n            >\n                <b-form-select\n                    id=\"input-8\"\n                    v-model=\"form.tipo_consulta\"\n                    :options=\"optionsFormSelect\"\n                    required\n                ></b-form-select>\n            </b-form-group>\n\n            <div class=\"mt-3\">\n                <b-button\n                    class=\"mt-4\"\n                    :disabled=\"buttonDisabled\"\n                    type=\"submit\"\n                    variant=\"primary bg-success\"\n                    >Guardar Registro</b-button\n                >\n            </div>\n        </b-form>\n    </div>\n</template>\n\n<script>\nimport { requestHandler } from '@/helpers/requestHandler'\nimport { requestOptions } from '@/helpers/requestOptions'\n\nexport default {\n    name: 'Form',\n    data() {\n        return {\n            form: {\n                //carnet: \"\",\n                expedient: '',\n                carnet: '',\n                expediente: '',\n                fecha: '',\n                nombre: '',\n                apellido: '',\n                edad: '',\n                temperatura: '',\n                direccion: '',\n                celular: '',\n                telefono_casa: '',\n                tipo_consulta: ''\n            },\n            show: true,\n            buttonDisabled: false,\n            toastVisible: false,\n            optionsFormSelect: [\n                { value: null, disabled: true, text: 'Seleccione un tipo de Clínica' },\n                { value: 'general', text: 'Medicina General' },\n                { value: 'oftalmologia', text: 'Oftalmología' },\n                { value: 'fisioterapia', text: 'Fisioterapia' },\n                { value: 'odontologia', text: 'Odontología' },\n                { value: 'audiologia', text: 'Audiología' },\n                { value: 'ecocardiograma', text: 'Ecocardiograma' }\n            ],\n            resultsname: ''\n        }\n    },\n    methods: {\n        async onSubmit(event) {\n            event.preventDefault()\n            this.buttonDisabled = true\n            this.toastVisible = true\n            console.log(this.form)\n            const reconsultation = await requestHandler\n                .async_fetch(\n                    `${process.env.VUE_APP_BACKEND}/api/v1/oldconsultation`,\n                    requestOptions.post(this.form)\n                )\n                .then(() => {\n                    this.showToast()\n                    this.showToastS()\n                    this.resultsname = `${this.form.nombre + this.form.apellido}`\n                })\n\n            //const reconsultationpdf = await this.exportpdf(this.form)\n            console.log(reconsultation)\n            //console.log(reconsultationpdf)\n            ;(this.form.carnet = ''),\n                (this.form.expediente = ''),\n                (this.form.fecha = ''),\n                (this.form.nombre = ''),\n                (this.form.apellido = ''),\n                (this.form.edad = ''),\n                (this.form.temperatura = ''),\n                (this.form.direccion = ''),\n                (this.form.celular = ''),\n                (this.form.telefono_casa = ''),\n                (this.form.tipo_consulta = ''),\n                // Trick to reset/clear native browser form validation state\n                (this.show = false)\n            this.$nextTick(() => {\n                this.show = true\n            })\n        },\n        onReset(event) {\n            event.preventDefault()\n            // Reset our form values\n            ;(this.form.carnet = ''),\n                (this.form.expediente = ''),\n                (this.form.fecha = ''),\n                (this.form.nombre = ''),\n                (this.form.apellido = ''),\n                (this.form.edad = ''),\n                (this.form.temperatura = ''),\n                (this.form.direccion = ''),\n                (this.form.celular = ''),\n                (this.form.telefono_casa = ''),\n                (this.form.tipo_consulta = ''),\n                (this.form.type = ''),\n                // Trick to reset/clear native browser form validation state\n                (this.show = false)\n            this.$nextTick(() => {\n                this.show = true\n            })\n        },\n        /*exportpdf(datatoprint) {\n            const a = document.createElement('a')\n            document.body.appendChild(a)\n            a.style = 'display: none'\n\n            fetch(\n                `${process.env.VUE_APP_BACKEND}/api/v1/newconsultations/pdf`,\n                requestOptions.post(datatoprint)\n            )\n                .then((res) => res.arrayBuffer())\n                .then((data) => {\n                    const blob = new Blob([data], { type: 'application/pdf' })\n                    this.fileCSVBlob = window.URL.createObjectURL(blob)\n                    a.href = this.fileCSVBlob\n                    a.download = `consulta-${this.resultsname}.pdf`\n                    a.click()\n                    window.URL.revokeObjectURL(this.fileCSVBlob)\n                    this.buttonDisabled = false\n                    this.toastVisible = false\n                    this.$bvToast.hide('messagePDF')\n                })\n                .catch((err) => {\n                    this.buttonDisabled = false\n                    this.toastVisible = false\n                    this.$bvToast.hide('messagePDF')\n                    console.log(err)\n                })\n        },*/\n        showToast() {\n            // Use a shorter name for this.$createElement\n            const h = this.$createElement\n            // Increment the toast count\n            // Create the message\n            const vNodesMsg = h('p', { class: ['text-center', 'mb-0'] }, [\n                h('strong', 'Generando PDF...  '),\n                h('b-spinner', { props: { small: true } })\n            ])\n            // Pass the VNodes as an array for message and title\n            this.$bvToast.toast([vNodesMsg], {\n                id: 'messagePDF',\n                solid: true,\n                variant: 'success',\n                toaster: 'b-toaster-bottom-right',\n                noCloseButton: true,\n                noAutoHide: true,\n                visible: this.toastVisible\n            })\n        },\n        showToastS() {\n            // Use a shorter name for this.$createElement\n            const h = this.$createElement\n            // Increment the toast count\n            // Create the message\n            const vNodesMsg = h('p', { class: ['text-center', 'mb-0'] }, [\n                h('strong', 'Consulta Ingresada con Éxito')\n            ])\n            // Pass the VNodes as an array for message and title\n            this.$bvToast.toast([vNodesMsg], {\n                solid: true,\n                variant: 'info',\n                toaster: 'b-toaster-top-right',\n                noCloseButton: true\n            })\n        }\n    },\n    computed: {\n        nameState() {\n            return this.form.cellphone.length == 8 ? true : false\n        }\n    }\n}\n/**\n * style=\"background-color:#6E6658; padding:3rem;\"\n * https://codepen.io/wfigueiredo/pen/BVmzzy\n */\n</script>\n"]}]}