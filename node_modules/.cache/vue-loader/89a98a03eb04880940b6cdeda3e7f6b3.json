emplate>\n  <div class=\"m-3\">\n    <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\n      <b-row cols=\"1\" cols-sm=\"1\" cols-md=\"1\" cols-lg=\"2\" cols-xl=\"2\">\n        <b-col\n          ><b-form-group\n            id=\"input-group-1\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"No. Factura\"\n            label-for=\"input-1\"\n          >\n            <b-form-input\n              id=\"input-1\"\n              v-model=\"form.bill_no\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n        <b-col\n          ><b-form-group\n            id=\"input-group-2\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Serie de Factura\"\n            label-for=\"input-2\"\n          >\n            <b-form-input\n              id=\"input-2\"\n              v-model=\"form.bill_series\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n      </b-row>\n      <b-row cols=\"1\" cols-sm=\"1\" cols-md=\"1\" cols-lg=\"2\" cols-xl=\"2\">\n        <b-col\n          ><b-form-group\n            id=\"input-group-3\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"NIT Proveedor\"\n            label-for=\"input-3\"\n          >\n            <b-form-input\n              id=\"input-3\"\n              v-model=\"form.provider_nit\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n        <b-col\n          ><b-form-group\n            id=\"input-group-4\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Nombre Proveedor\"\n            label-for=\"input-4\"\n          >\n            <b-form-input\n              id=\"input-4\"\n              v-model=\"form.provider_name\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n      </b-row>\n      <b-row cols=\"1\" cols-sm=\"1\" cols-md=\"1\" cols-lg=\"2\" cols-xl=\"2\">\n        <b-col\n          ><b-form-group\n            id=\"input-group-5\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Valor\"\n            label-for=\"input-5\"\n          >\n            <b-form-input\n              id=\"input-5\"\n              v-model=\"form.value\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n        <b-col\n          ><b-form-group\n            id=\"input-group-6\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Área\"\n            label-for=\"input-6\"\n          >\n            <b-form-input\n              id=\"input-6\"\n              v-model=\"form.area\"\n              placeholder=\"\"\n              required\n            ></b-form-input> </b-form-group\n        ></b-col>\n      </b-row>\n      <b-row>\n        <b-col>\n          <b-form-group\n            id=\"input-group-7\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Descripción\"\n            label-for=\"input-7\"\n          >\n            <b-form-input\n              id=\"input-7\"\n              v-model=\"form.description\"\n              placeholder=\"\"\n              required\n            ></b-form-input>\n          </b-form-group>\n        </b-col>\n      </b-row>\n      <b-row cols=\"1\" cols-sm=\"1\" cols-md=\"1\" cols-lg=\"2\" cols-xl=\"2\">\n        <b-col>\n          <b-form-group\n            id=\"input-group-8\"\n            label-align=\"left\"\n            label-align-sm=\"left\"\n            label-align-lg=\"left\"\n            label-align-md=\"left\"\n            label-align-xl=\"left\"\n            label=\"Fecha\"\n            label-for=\"input-8\"\n          >\n            <div>\n              <b-form-datepicker\n                id=\"input-8\"\n                v-model=\"form.date\"\n                label-no-date-selected=\"Seleccione una Fecha\"\n                required\n                locale=\"es\"\n              ></b-form-datepicker>\n            </div> </b-form-group\n        ></b-col>\n        <b-col> </b-col>\n      </b-row>\n\n      <div class=\"mt-3\">\n        <b-button\n          :disabled=\"buttonDisabled\"\n          type=\"submit\"\n          variant=\"primary bg-success\"\n          class=\"mt-4\"\n          style=\"min-width: 14rem\"\n          >Editar Compra\n        </b-button>\n      </div>\n    </b-form>\n  </div>\n</template>\n\n<script>\nimport { requestHandler } from \"@/helpers/requestHandler\";\nimport { requestOptions } from \"@/helpers/requestOptions\";\nimport EventBus from \"@/bus\";\n\nexport default {\n  name: \"Form\",\n  data() {\n    return {\n      form: {\n        bill_no: \"\",\n        bill_series: \"\",\n        provider_nit: \"\",\n        provider_name: \"\",\n        value: \"\",\n        description: \"\",\n        area: \"\",\n        date: \"\",\n      },\n      show: true,\n      buttonDisabled: false,\n    };\n  },\n  methods: {\n    async onSubmit(event) {\n      event.preventDefault();\n      this.buttonDisabled = true;\n      console.log(this.form);\n      this.form.value = this.form.value.replace(/,/g, \"\");\n      const reconsultation = await requestHandler\n        .async_fetch(\n          `${process.env.VUE_APP_BACKEND}/api/v1/bills`,\n          requestOptions.put(this.form)\n        )\n        .then(() => {\n          this.showToast();\n          this.buttonDisabled = false;\n        });\n      EventBus.$emit(\"updateCompraTable\");\n      console.log(reconsultation);\n      (this.form.bill_no = \"\"),\n        (this.form.bill_series = \"\"),\n        (this.form.provider_nit = \"\"),\n        (this.form.provider_name = \"\"),\n        (this.form.value = \"\"),\n        (this.form.area = \"\"),\n        (this.form.description = \"\"),\n        (this.form.date = \"\"),\n        // Trick to reset/clear native browser form validation state\n        (this.show = false);\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      (this.form.bill_no = \"\"),\n        (this.form.bill_series = \"\"),\n        (this.form.provider_nit = \"\"),\n        (this.form.provider_name = \"\"),\n        (this.form.value = \"\"),\n        (this.form.area = \"\"),\n        (this.form.description = \"\"),\n        (this.form.date = \"\"),\n        // Trick to reset/clear native browser form validation state\n        (this.show = false);\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n    showToast() {\n      // Use a shorter name for this.$createElement\n      const h = this.$createElement;\n      // Increment the toast count\n      // Create the message\n      const vNodesMsg = h(\"p\", { class: [\"text-center\", \"mb-0\"] }, [\n        h(\"strong\", \"Compra Editada con Éxito\"),\n      ]);\n      // Pass the VNodes as an array for message and title\n      this.$bvToast.toast([vNodesMsg], {\n        solid: true,\n        variant: \"info\",\n        toaster: \"b-toaster-top-right\",\n        noCloseButton: true,\n      });\n    },\n    updateData(item) {\n      this.form = item;\n      let fecha = item.date.split(\"/\");\n      const dia = fecha[0];\n      const mes = fecha[1];\n      const ano = fecha[2];\n      this.form.date = `${ano}-${mes}-${dia}`;\n      console.log(item.bill_no, fecha);\n    },\n  },\n  async created() {\n    EventBus.$on(\"editCompraTable\", (item) => {\n      console.log(\"evento\", item);\n      this.updateData(item);\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    });\n  },\n  beforeDestroy() {\n    EventBus.$off(\"editCompraTable\");\n  },\n};\n/**\n * style=\"background-color:#6E6658; padding:3rem;\"\n * https://codepen.io/wfigueiredo/pen/BVmzzy\n */\n</script>"]}]}